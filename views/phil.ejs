<% layout("layouts/boilerplate") -%>
<section class="jumbo__section">
    <div class="jumbotron jumbo--phil container">
        <div class="jumbo__cont">
            <div class="jumbo__group">
                <h2 class="jumbo__title mb-3">Phil Lynott</h2>
                <p class="jumbo__date mb-3">1949 - 1986</p>
            </div>
            <form action="/phildisc" method="GET" id="philDisc">
                <input type="submit" class="btn btn-md text-center basebtn basebtn--jumbo" form="philDisc" role="button" value="Discography">
            </form>
        </div>
    </div>
    <div class="style-line"></div>
    <div class="quote container">
        <div class="media">
            <div class="media-body">
                <blockquote class="blockquote text-center">
                    <p class="quote__p">"Why did God take such a creative person?"</p>
                    <footer class="blockquote-footer">James Hetfield of Metallica </footer>
                </blockquote>
            </div>
            <img src="/img/het1.jpg" class="ml-3 quote__img" alt="james hetfield">
        </div>
    </div>
    <div class="style-line"></div>
</section>
<article>
    <div class="container">
        <h2 class="text-center pl-4 pr-4 base__title">Biography</h2>
    </div>
    <div class="container biog__container">
        <div class="row">
            <div class="col-12 col-lg-6 d-flex align-self-stretch">
                <table class="table table-borderless 
             mt-5 biog__table">
                    <tbody>
                        <tr>
                            <th colspan="2" class="text-center biog__title biog__title--stat">Vital Statistics</th>
                        </tr>
                        <tr class="biog__info">
                            <th scope="row" class="text-center">Born</th>
                            <td>Phil Parris Lynott</td>
                            <td>August 20, 1949</td>
                            <td>West Bromwich, Staffs, England</td>
                        </tr>
                        <tr class="biog__info">
                            <th scope="row" class="text-center">Died</th>
                            <td>January 4, 1986(aged 36)</td>
                            <td>Salisbury, Wiltshire, England</td>
                        </tr>
                        <tr class="biog__info">
                            <th scope="row" class="text-center">Cause of death</th>
                            <td>Pneumonia and heart failure due to drug overdose.</td>
                        </tr>
                        <tr class="biog__info">
                            <th scope="row" class="text-center">Resting place</th>
                            <td>St. Fintan's Cemetery</td>
                            <td>Sutton, County Dublin, Ireland</td>
                        </tr>
                        <tr class="biog__info">
                            <th scope="row" class="text-center">Ex-partner</th>
                            <td>Caroline Crowther</td>
                        </tr>
                        <tr class="biog__info">
                            <th scope="row" class="text-center">Occupation</th>
                            <td>Musician</td>
                        </tr>
                        <tr class="biog__info">
                            <th scope="row" class="text-center">Instruments</th>
                            <td>Bass guitar</td>
                            <td>Lead vocals</td>
                        </tr>
                        <tr class="biog__info">
                            <th scope="row" class="text-center">Genre</th>
                            <td>Heavy Metal</td>
                            <td>Hard Rock</td>
                            <td>Celtic Rock</td>
                            <td>Blues Rock</td>
                        </tr>
                        <tr class="biog__info">
                            <th scope="row" class="text-center">Labels</th>
                            <td>Vertigo</td>
                            <td>Mercury</td>
                            <td>Warner Bros</td>
                        </tr>
                    </tbody>
                </table>
            </div>
            <div class=" col-lg-6 mt-5 biog__table biog__table--text">
                <P class="biog__text">
